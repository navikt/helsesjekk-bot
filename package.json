{
    "name": "helsesjekk",
    "version": "1.0.0",
    "license": "MIT",
    "packageManager": "yarn@4.12.0",
    "engines": {
        "node": "24"
    },
    "scripts": {
        "dev": "next dev | pino-pretty --messageKey=message",
        "dev:db": "docker run -e POSTGRES_PASSWORD=postgres -p 5432:5432 -it postgres:14",
        "build": "next build",
        "start": "next start",
        "lint": "eslint src",
        "test": "bun test",
        "start:migrate": "yarn prisma:migrate && yarn start",
        "prisma:generate": "prisma generate && yarn prettier --write src/db/generated/**/*.{ts,mjs}",
        "prisma:migrate": "NODE_OPTIONS='-r next-logger' prisma migrate deploy",
        "prisma:migrate-dev": "prisma migrate dev",
        "prisma:seed": "bun run seed/seed.ts | pino-pretty --messageKey=message",
        "postinstall": "command -v husky && husky || true"
    },
    "dependencies": {
        "@navikt/aksel-icons": "^7.34.0",
        "@navikt/ds-css": "^7.34.0",
        "@navikt/ds-react": "^7.34.0",
        "@navikt/ds-tailwind": "^7.34.0",
        "@navikt/next-logger": "^4.4.1",
        "@navikt/oasis": "^4.1.1",
        "@prisma/adapter-pg": "^7.0.0",
        "@prisma/client": "7.0.0",
        "@slack/bolt": "^4.6.0",
        "@slack/logger": "^4.0.0",
        "@slack/types": "^2.19.0",
        "clsx": "^2.1.1",
        "date-fns": "^4.1.0",
        "dotenv": "^17.2.3",
        "next": "^16.0.7",
        "next-logger": "^5.0.2",
        "nextleton": "^0.6.1",
        "node-cron": "^4.2.1",
        "nuqs": "^2.8.1",
        "pino": "^10.1.0",
        "postcss": "^8.5.6",
        "prisma": "7.0.0",
        "react": "19.2.1",
        "react-dom": "19.2.1",
        "recharts": "3.1.2",
        "remeda": "^2.32.0",
        "sharp": "^0.34.5",
        "tailwind-merge": "^3.4.0",
        "zod": "^4.1.12"
    },
    "devDependencies": {
        "@navikt/tsm-eslint-react": "^1.0.0",
        "@navikt/tsm-prettier": "^2.0.0",
        "@tailwindcss/postcss": "^4.1.17",
        "@types/bun": "^1.3.3",
        "@types/node": "^24.10.1",
        "@types/node-cron": "^3.0.11",
        "@types/react": "19.2.6",
        "@vercel/nft": "^1.1.1",
        "eslint": "^9.39.1",
        "eslint-config-next": "16.0.7",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-prettier": "^5.5.4",
        "husky": "^9.1.7",
        "lint-staged": "^16.2.7",
        "pino-pretty": "^13.1.2",
        "prettier": "^3.6.2",
        "prettier-plugin-prisma": "^5.0.0",
        "prettier-plugin-tailwindcss": "^0.7.1",
        "tailwindcss": "^4.1.17",
        "typescript": "^5.9.3"
    },
    "prettier": "@navikt/tsm-prettier"
}
