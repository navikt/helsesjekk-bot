{
    "name": "helsesjekk",
    "version": "1.0.0",
    "license": "MIT",
    "packageManager": "yarn@4.12.0",
    "engines": {
        "node": "24"
    },
    "scripts": {
        "dev": "next dev | pino-pretty --messageKey=message",
        "dev:db": "docker run -e POSTGRES_PASSWORD=postgres -p 5432:5432 -it postgres:14",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "test": "bun test",
        "start:migrate": "yarn prisma:migrate && yarn start",
        "prisma:generate": "prisma generate",
        "prisma:migrate": "NODE_OPTIONS='-r next-logger' prisma migrate deploy",
        "prisma:migrate-dev": "prisma migrate dev",
        "prisma:seed": "bun run seed/seed.ts | pino-pretty --messageKey=message",
        "postinstall": "command -v husky && husky || true"
    },
    "dependencies": {
        "@navikt/aksel-icons": "^7.33.5",
        "@navikt/ds-css": "^7.33.5",
        "@navikt/ds-react": "^7.33.5",
        "@navikt/ds-tailwind": "^7.33.5",
        "@navikt/next-logger": "^4.4.1",
        "@navikt/oasis": "^4.1.1",
        "@prisma/client": "6.15.0",
        "@slack/bolt": "^4.4.0",
        "@slack/logger": "^4.0.0",
        "@slack/types": "^2.16.0",
        "clsx": "^2.1.1",
        "date-fns": "^4.1.0",
        "next": "^15.5.2",
        "next-logger": "^5.0.1",
        "nextleton": "^0.6.1",
        "node-cron": "^4.2.1",
        "nuqs": "^2.6.0",
        "pino": "^9.9.4",
        "postcss": "^8.5.6",
        "prisma": "6.15.0",
        "react": "^19.1.1",
        "react-dom": "^19.1.1",
        "recharts": "^3.1.2",
        "remeda": "^2.30.0",
        "sharp": "^0.34.3",
        "tailwind-merge": "^3.3.1",
        "zod": "^4.1.5"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3.3.1",
        "@navikt/eslint-config-teamsykmelding": "^8.1.1",
        "@tailwindcss/postcss": "^4.1.13",
        "@types/bun": "^1.2.21",
        "@types/node": "^24.10.1",
        "@types/node-cron": "^3.0.11",
        "@types/react": "19.1.12",
        "eslint": "^9.35.0",
        "eslint-config-next": "^15.5.2",
        "husky": "^9.1.7",
        "lint-staged": "^16.1.6",
        "pino-pretty": "^13.1.1",
        "prettier": "^3.6.2",
        "prettier-plugin-prisma": "^5.0.0",
        "prettier-plugin-tailwindcss": "^0.6.14",
        "tailwindcss": "^4.1.13",
        "typescript": "^5.9.2"
    },
    "prettier": "@navikt/eslint-config-teamsykmelding/prettier",
    "eslintConfig": {
        "extends": [
            "@navikt/eslint-config-teamsykmelding",
            "next/core-web-vitals"
        ]
    }
}
