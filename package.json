{
    "name": "helsesjekk",
    "version": "1.0.0",
    "license": "MIT",
    "packageManager": "yarn@4.9.2",
    "engines": {
        "node": "22"
    },
    "scripts": {
        "dev": "next dev | pino-pretty",
        "dev:db": "docker run -e POSTGRES_PASSWORD=postgres -p 5432:5432 -it postgres:14",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "test": "bun test",
        "start:migrate": "yarn prisma:migrate && yarn start",
        "prisma:generate": "prisma generate",
        "prisma:migrate": "NODE_OPTIONS='-r next-logger' prisma migrate deploy",
        "prisma:migrate-dev": "prisma migrate dev",
        "prisma:seed": "bun run seed/seed.ts | pino-pretty",
        "postinstall": "command -v husky && husky || true"
    },
    "dependencies": {
        "@navikt/aksel-icons": "^7.25.1",
        "@navikt/ds-css": "^7.25.1",
        "@navikt/ds-react": "^7.25.1",
        "@navikt/ds-tailwind": "^7.25.1",
        "@navikt/next-logger": "^4.1.5",
        "@navikt/oasis": "^3.8.0",
        "@prisma/client": "6.12.0",
        "@slack/bolt": "^4.4.0",
        "@slack/logger": "^4.0.0",
        "@slack/types": "^2.15.0",
        "clsx": "^2.1.1",
        "date-fns": "^4.1.0",
        "next": "^15.4.4",
        "next-logger": "^5.0.1",
        "nextleton": "^0.6.1",
        "node-cron": "^4.2.1",
        "nuqs": "^2.4.3",
        "pino": "^9.7.0",
        "postcss": "^8.5.6",
        "prisma": "6.12.0",
        "react": "^19.1.0",
        "react-dom": "^19.1.0",
        "recharts": "^2.15.4",
        "remeda": "^2.26.1",
        "sharp": "^0.34.3",
        "tailwind-merge": "^3.3.1",
        "uuid": "^11.1.0",
        "zod": "^4.0.10"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3.3.1",
        "@navikt/eslint-config-teamsykmelding": "^8.0.0",
        "@tailwindcss/postcss": "^4.1.11",
        "@types/bun": "^1.2.19",
        "@types/node": "^22.16.5",
        "@types/node-cron": "^3.0.11",
        "@types/react": "19.1.8",
        "@types/uuid": "^10.0.0",
        "eslint": "^9.32.0",
        "eslint-config-next": "^15.4.4",
        "husky": "^9.1.7",
        "lint-staged": "^16.1.2",
        "pino-pretty": "^13.1.1",
        "prettier": "^3.6.2",
        "prettier-plugin-prisma": "^5.0.0",
        "prettier-plugin-tailwindcss": "^0.6.14",
        "tailwindcss": "^4.1.11",
        "typescript": "^5.8.3"
    },
    "prettier": "@navikt/eslint-config-teamsykmelding/prettier",
    "eslintConfig": {
        "extends": [
            "@navikt/eslint-config-teamsykmelding",
            "next/core-web-vitals"
        ]
    }
}
